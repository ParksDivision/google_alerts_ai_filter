{
  "name": "rss-content-analyzer",
  "version": "1.0.0",
  "description": "A secure tool for scraping and analyzing RSS feed content using Claude",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "node dist/index.js",
    "dev": "ts-node --esm src/index.ts",
    "lint": "eslint --ext .ts src/",
    "analyze": "node dist/index.js analyze",
    "serve": "node dist/index.js serve",
    "process-rss": "node dist/index.js process-rss",
    "run-all": "node dist/index.js run-all"
  },
  "keywords": [
    "rss",
    "scraper",
    "claude",
    "ai",
    "analysis"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.19.0",
    "@fast-csv/format": "^4.3.5",
    "cheerio": "1.0.0-rc.12",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "exponential-backoff": "^3.1.1",
    "fast-csv": "^5.0.0",
    "node-fetch": "^3.3.2",
    "p-limit": "^5.0.0",
    "p-queue": "^8.0.1",
    "rss-parser": "^3.13.0",
    "sanitize-html": "^2.11.0",
    "turndown": "^7.1.2",
    "undici": "^6.6.2",
    "xlsx-js-style": "^1.2.0",
    "yargs": "^17.7.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/sanitize-html": "^2.9.5",
    "@types/turndown": "^5.0.4",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}